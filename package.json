{
  "name": "material-bank",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"nodemon backend/index.js\" \"cd frontend && vite\"",
    "build": "cd frontend && vite build",
    "lint": "eslint .",
    "lint:frontend": "eslint frontend/**",
    "lint:backend": "eslint backend/**",
    "preview": "cd frontend && vite preview",
    "test:frontend": "vitest run",
    "test:backend": "vitest run --config vite.config.backend.mjs",
    "test": "concurrently \"npm run test:frontend\" \"npm run test:backend\"",
    "test:e2e": "cross-env NODE_ENV=test npx playwright test",
    "dev:backend:test": "cross-env NODE_ENV=test nodemon backend/index.js",
    "dev:frontend:test": "cross-env NODE_ENV=development vite --mode test",
    "dev:test": "concurrently \"npm run dev:backend:test\" \"npm run dev:frontend:test\"",
    "coverage:frontend": "vitest run --coverage",
    "coverage:backend": "vitest run --config vite.config.backend.mjs --coverage",
    "test:coverage": "concurrently \"npm run coverage:frontend\" \"npm run coverage:backend\"",
    "format": "prettier --write .",
    "start": "node backend/index.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.8.3",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dompurify": "^3.2.6",
    "dotenv": "^16.6.1",
    "express": "^4.21.1",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "material-bank": "file:",
    "mime": "^4.0.7",
    "multer": "^2.0.2",
    "pg": "^8.14.1",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "sequelize": "^6.37.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.25.9",
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.54.1",
    "@stylistic/eslint-plugin-js": "^2.10.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.1.1",
    "codecov": "^3.8.2",
    "concurrently": "^9.0.1",
    "coveralls": "^3.1.1",
    "cross-env": "^10.0.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "json-serve": "^0.1.0",
    "lcov-result-merger": "^5.0.1",
    "nodemon": "^3.1.7",
    "playwright": "^1.54.1",
    "prettier": "^3.3.3",
    "supertest": "^7.1.1",
    "vite": "^6.3.4",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": "20.x"
  }
}
